{
    "name": "fueled-backend-assessment",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "clean": "rm -rf ./build/*",
        "build:ts": "tsc",
        "build": "npm run clean && npm run build:types && npm run build:js",
        "build:types": "tsc --emitDeclarationOnly",
        "build:js": "babel src --out-dir build --extensions \".ts,.js\" --source-maps inline --copy-files",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "test": "jest --runInBand --verbose --coverage --detectOpenHandles",
        "start": "node ./build/index.js",
        "start:dev": "nodemon --inspect=9000",
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check -- --watch",
        "pretest": "npm run lint && NODE_ENV=test npm run migrate:undo:all && NODE_ENV=test npm run migrate",
        "prestart": "npm run migrate",
        "migrate:undo:all": "sequelize db:migrate:undo:all",
        "seed": "sequelize db:seed:all",
        "seed:undo:all": "sequelize db:seed:undo:all",
        "migrate": "sequelize db:migrate"
    },
    "repository": {
        "type": "git",
        "url": "git+git@github.com:fidelisojeah/fueled.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/fidelisojeah/fueled/issues"
    },
    "homepage": "https://github.com/fidelisojeah/fueled#readme",
    "dependencies": {
        "bcrypt": "^5.0.0",
        "compression": "^1.7.4",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-winston": "^4.0.5",
        "helmet": "^3.23.3",
        "http-status-codes": "^1.4.0",
        "morgan": "^1.10.0",
        "pg": "^8.3.0",
        "pg-hstore": "^2.3.3",
        "sequelize": "^6.3.4",
        "sequelize-cli": "^6.2.0",
        "swagger-ui-express": "^4.1.4",
        "uuid": "^8.3.0",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.5",
        "@babel/core": "^7.11.1",
        "@babel/node": "^7.10.5",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-typescript": "^7.10.4",
        "@babel/register": "^7.10.5",
        "@types/compression": "^1.7.0",
        "@types/eslint": "^6.8.1",
        "@types/express": "^4.17.7",
        "@types/faker": "^4.1.12",
        "@types/helmet": "0.0.46",
        "@types/jest": "^25.2.3",
        "@types/morgan": "^1.9.1",
        "@types/node": "^13.13.15",
        "@types/supertest": "^2.0.10",
        "@types/swagger-ui-express": "^4.1.2",
        "@types/validator": "^13.1.0",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "babel-jest": "^26.2.2",
        "babel-plugin-module-resolver": "^4.0.0",
        "chai": "^4.2.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "faker": "Marak/faker.js",
        "husky": "^4.2.5",
        "jest": "^25.5.4",
        "jest-junit": "^10.0.0",
        "nodemon": "^2.0.4",
        "prettier": "^2.0.5",
